{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/ChristopherCohen007/3179/main/data/alcohol_data_averages.csv"
  },
  "mark": {"type": "line", "point": true},
  "title": "Average Comsumption of all Countries Over Time",
  "width": 800,
  "height": 300,
  "encoding": {
    "x": {"field": "year", "type": "ordinal", "title": ""},
    "y": {
      "field": "avg_comsumption",
      "type": "quantitative",
      "scale": {"domain": [6.1, 6.6]},
      "title": "Average Comsumption"
    }
  },
  "config": {}
}
